   <%= form_for @project.prelationships.build(:pfollowed_id =>
		                                      @project_id) do |f| %>
   <% Plant.all.each do |plant| %>  
   <div class ="gardenselect">
   <%= image_tag plant.icon_image %>
   <%= check_box_tag "prelationship[pfollower_id][]", plant.id, false, :style => "display: none;" %> 
   </div>
   <% end %>  
   <%= hidden_field_tag :project_id, @project.id %> 
<%= f.submit "Start Your Garden" %>
<% end %> 